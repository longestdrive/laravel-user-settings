{
    "name": "grimthorr/laravel-user-settings",
    "description": "Simple user settings facade for Laravel 6. Forked from Grimthorr/laravel-user-settings",
    "homepage": "https://github.com/longestdrive/laravel-user-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Longestdrive",
            "email": "ray@raycraneweb.co.uk",
            "homepage": "http://raycraneweb.co.uk"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "illuminate/support": "6.*",
        "illuminate/config": "6.*",
        "illuminate/database": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Longestdrive\\LaravelUserSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Longestdrive\\LaravelUserSettings\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Longestdrive\\LaravelUserSettings\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
