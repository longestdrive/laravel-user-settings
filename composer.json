{
    "name": "longestdrive/laravel-user-settings",
    "description": "Simple user settings facade for Laravel 9. Forked from Grimthorr/laravel-user-settings",
    "homepage": "https://github.com/longestdrive/laravel-user-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Longestdrive",
            "email": "ray@raycraneweb.co.uk",
            "homepage": "http://raycraneweb.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "longestdrive/laravel-user-settings",
                "version": "10.0.1",
                "source": {
                    "url": "git://github.com/longestdrive/laravel-user-settings.git",
                    "type": "git",
                    "reference": "10.0.1"
                },
                "autoload": {
                    "psr-0" : {
                        "Longestdrive\\DangerZone" : "src"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-json": "*",
        "illuminate/support": "10.*",
        "illuminate/config": "10.*",
        "illuminate/database": "10.*"
    },
    "autoload": {
        "psr-4": {
            "Longestdrive\\LaravelUserSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Longestdrive\\LaravelUserSettings\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Longestdrive\\LaravelUserSettings\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
